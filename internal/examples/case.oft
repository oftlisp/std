(module std/internal/examples/case
  main)

(defn range (lower upper)
  (if (= lower upper)
    nil
    (cons lower (range (1+ lower) upper))))

(defn number-name (n)
  (case n
    0 "zero"
    1 "one"
    2 "two"
    3 "three"
    4 "four"
    5 "five"
    6 "six"
    7 "seven"
    8 "eight"
    9 "nine"
      "unknown"))

(defn main (args)
  (for-each x (range 0 11)
    (println x ": " (number-name x))))
