(module std/internal/examples/oop
  main)

(defclass range
  (val start)
  (val end)

  (ctor (s e)
    (list s e))
  (named-ctor empty ()
    (range 0 0))

  (defn size ()
    (- end start))
  (defn to-string ()
    (concat-strings "(" (show start) " - " (show end) ")")))

(defclass rectangle
  (val height)
  (val width)

  (ctor (h w)
    (list h w))

  (defn area ()
    (* height width)))

(defn square (s)
  (rectangle s s))

(defn main (args)
  (def x (range 1 3))
  (println x)
  (println (range.size x))
  
  (println (rectangle 1 2))
  (println (square 3)))
