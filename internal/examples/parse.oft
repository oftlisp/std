(module std/internal/examples/parse
  main)

(import std/parsing/lexer
  lex-string)

(defn main (args)
  (def terminals '(
    (simple "+" +)
    (simple "-" -)
    (simple "*" *)
    (simple "/" /)
    (simple "(" open)
    (simple ")" close)
    (regex "[0-9]+" num)
    (regex "[a-zA-Z]+" var)
    (ignore regex " +")))
  (def input "(1 + 2 * 3 + 4) * 5")
  (println (lex-string terminals input)))
