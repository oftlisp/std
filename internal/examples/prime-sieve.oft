(module std/internal/examples/prime-sieve
  main)

(import std/lazy
  naturals
  stream-cons)

(defn sieve (stream)
  (stream-cons
    (head stream)
    (sieve (filter
      (fn (x) (/= (mod x (head stream)) 0))
      (tail stream)))))

(def primes (sieve (skip 2 naturals)))

(defn main ()
  (last (map println primes)))
