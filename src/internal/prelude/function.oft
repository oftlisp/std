(module std/internal/prelude/function
  [. const0 const1 fix flip id]
  no-prelude)

(intrinsics:defn . (f g)
  \(f (g $)))

(intrinsics:defn const0 (x)
  (intrinsics:fn () x))

(intrinsics:defn const1 (x)
  (intrinsics:fn (_) x))

(intrinsics:defn fix (f)
  (intrinsics:fn (x) (f (fix f) x)))

(intrinsics:defn flip (f)
  (intrinsics:fn (a b)
    (f b a)))

(intrinsics:defn id (x) x)
